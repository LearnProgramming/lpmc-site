.user_info_block:
	border-radius: 20px
	background-color: #fff
	width: 400px
	padding: 15px
	box-shadow: 0px 0px 10px 5px rgba(0,0,0,0.15)

	.github_info:
		img:
			position: relative
			top: 5px
			padding: 8px
			width: 64px
			height: 64px
			float: left

		span:
			padding-right: 15px
			display: block
