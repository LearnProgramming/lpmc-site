html:
	position: relative
	min-height: 100%

body:
	background-color: #dcdcdc
	margin-bottom: 100px

.center:
	text-align: center

#footer:
	position: absolute
	bottom: 0
	width: 100%
	height: 60px
	background-color: #95a5a6
	color: white
	padding-top: 20px
	text-align: center

.jumbotron:
	background-color: #fdfdfd
	box-shadow: inset 0 0 10px #888

	h1:
		font-size: 38px
		margin-bottom: 38px

.well:
	background-color: #fdfdfd
	box-shadow: inset 0 0 6px #888
	border: none

.navbar:

	img:
		float: left
		width: 32px
		height: 32px
		margin-right: 5px
		margin-top: 15px
		vertical-align: middle
		display: inline
		border-radius: 50%
		border: 2px solid #fff

	#nav-dropdown:
		float: right

.form-control:
	border: 1px solid #bcbcbc

